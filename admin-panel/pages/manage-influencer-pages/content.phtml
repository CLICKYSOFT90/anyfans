                ((''+day).length<2 ? '0' : '') + day;
            var attr = $(this).data('add');
            var copied_div = '<div class="row">'+
                '<div class="col-md-12" id="normal-query-form">'+
                '<div class="form-group form-float">'+
                '<div class="form-line">'+
                '<input type="file" required class="form-control" name="gallery[]" accept="image/*">'+
